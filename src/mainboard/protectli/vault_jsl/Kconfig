    string
    default "PT201" if BOARD_PROTECTLI_PT201
    default "PT401" if BOARD_PROTECTLI_PT401
    default "PT601" if BOARD_PROTECTLI_PT601

config MAINBOARD_PART_NUMBER
    string
    default "PT201" if BOARD_PROTECTLI_PT201
    default "PT401" if BOARD_PROTECTLI_PT401
    default "PT601" if BOARD_PROTECTLI_PT601

config MAINBOARD_FAMILY
    string
    default "Protectli PTXXX"

config DEVICETREE
    string
    default "variants/baseboard/devicetree.cb"

